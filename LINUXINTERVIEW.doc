\pard\plain\ltrpar\ql\s29\itap0{\s29\f0\fs24\lang1033{\*\listtag0}\abinodiroverride\ltrch  6 --Execute onlyadminadmin}{\s29\f0\fs24\lang1033{\*\listtag0}\par}
