\pard\plain\ltrpar\ql\s29\itap0{\s29\f0\fs24\lang1033{\*\listtag0}\abinodiroverride\ltrch define(`confMAX_RCPTS_PER_MESSAGE',`50')dnl}{\s29\f0\fs24\lang1033{\*\listtag0}\par}
