\pard\plain\ltrpar\ql\s29\itap0{\s29\f0\fs24\lang1033{\*\listtag0}\abinodiroverride\ltrch #nmap networkID/subnetmask}{\s29\f0\fs24\lang1033{\*\listtag0}\par}
